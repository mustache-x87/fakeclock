{
  "version": "0.2.0",
  "configurations": [
    {

      "name": "CMake Build",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build~/debug/test_fakeclock",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/build~/debug",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "cmake: build",
      "miDebuggerPath": "/usr/bin/gdb",
      "logging": {
        "moduleLoad": false,
        "trace": false
      }
    }
  ]
}
